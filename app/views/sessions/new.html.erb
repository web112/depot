<h1>Welcome back</h1>
<div class = "depot_form">
	<%= form_tag do %>
	<fieldset>
		<legend>
			Please Log In
		</legend>
		<table>
			<tr>
				<td>
				<label for ="name">
					Username:
				</label>
				</td>
				<td><%= text_field_tag :name, params[:name] %></td>
			</tr>
			<tr>
				<td>
				<label for ="password">
					Password:
				</label>
				</td>
				<td><%= password_field_tag :password, params[:password] %></td>
			</tr>
			<tr>
				<td>

				</td>
				<td><%= submit_tag "Login", :class=>"button"%></td>
			</tr>
		</table>

	</fieldset>
	<%end%>
</div>
